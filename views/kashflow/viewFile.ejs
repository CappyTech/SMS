<div class="container my-5">
    <p>
        <a href="/kashflow/project/read/<%= uuid %>">
            Go Back to Project
        </a>
    </p>
    <% if (fileType === 'image') { %>
        <h2>Image</h2>
        <img src="<%= filePath %>" alt="<%= file%>" style="max-width: 100%; height: auto;">
    <% } else if (fileType === 'pdf') { %>
        <h2>PDF</h2>
        <!--
        <embed src="<%= filePath %>" type="application/pdf" width="100%" height="600px" />
        -->
        <iframe src="<%= filePath %>" width="100%" height="1220" style="border: none;"></iframe>
    <% } else { %>
        <p>Unsupported file type.</p>
    <% } %>
</div>