<% if (successMessage && successMessage.length > 0) { %>
    <div class="container">
        <br>
        <div class="alert alert-success alert-dismissible fade show p-3" role="alert">
            <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
            <%= successMessage %>
        </div>
    </div>
<% } %>